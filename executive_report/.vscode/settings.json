
{
    
    "code-runner.executorMap": {
        "latex": "./.vscode/latex_clean.sh"
    },
    "code-runner.cwd": "$workspaceRoot",

    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 5000,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.ist",
        "*.fls",
        "*.log",
        "*.fdb_latexmk"
    ],
    "cSpell.words": [
        "Coursera",
        "Diarization",
        "DIHARD",
        "discriminability",
        "EEND",
        "Pyannote",
        "regularisation",
        "specialisation",
        "visualised",
        "burstiness"
    ]
}




